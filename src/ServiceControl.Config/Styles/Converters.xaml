<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:converters="clr-namespace:ServiceControl.Config.Xaml.Converters">

    <converters:BoolToVisibilityConverter x:Key="boolToVis" />
    <converters:BoolToVisibilityConverter x:Key="boolToVisInverted" Invert="True" />
    <converters:BoolToVisibilityConverter x:Key="boolToVisHidden" IsHidden="True" />

    <converters:BoolToVisibilityConverter x:Key="boolToVisHiddenInverted"
                                          Invert="True"
                                          IsHidden="True" />

    <converters:StringNullOrEmptyToVisibilityConverter x:Key="nullOrEmptyToVis" />
</ResourceDictionary>