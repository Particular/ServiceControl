  <ItemGroup>
    <ProjectReference Include="..\ServiceControl.Audit.Persistence\ServiceControl.Audit.Persistence.csproj" />
  </ItemGroup>

  <ItemGroup>
    <None Update="RavenLicense.json">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
